{
  "workflow_info": {
    "title": "string", // 工作流名称
    "description": "string", // 工作流描述
    "userId": "string", // 对应用户id
    "serviceType": "string" // service类型，ml或graph
  },
  "nodes": [
    {
      "id": "string",
      // 组件唯一ID
      "name": "string",
      // 组件名称（英文）
      "chineseName": "string",
      // 组件名称（中文）
      "group": "string",
      // 组件类型（英文）
      "chineseGroup": "string",
      // 组件类型（中文）
      "serviceType": "string",
      // 服务类型（ml/graph）
      "mark": 0,
      // 同类型节点标记（从0开始）
      "position": [
        10,
        20
      ],
      // 节点位置 [x, y]
      "simpleAttributes": [
        // 简单属性列表
        {
          "name": "string",
          // 属性名
          "chineseName": "string",
          // 属性中文名
          "value": "string",
          // 属性值（字符串形式）
          "valueType": "string"
          // 属性类型（Int/Double/String/Boolean）
        }
      ],
      "complicatedAttributes": [
        // 复杂属性列表
        {
          "name": "string",
          // 属性名
          "chineseName": "string",
          // 属性中文名
          "value": {
            // JSON对象（任意结构）
            "key": "value"
          }
        }
      ],
      "inputAnchors": [
        // 输入锚点列表
        {
          "seq": 0,
          // 锚点序号（从左到右）
          "numOfConnectedEdges": 0,
          // 连接边数量（0或1）
          "sourceAnchor": {
            // 当numOfConnectedEdges>0时存在
            "nodeName": "string",
            // 源节点名称
            "nodeMark": 0,
            // 源节点标记
            "seq": 0
            // 源锚点序号
          }
        }
      ],
      "outputAnchors": [
        // 输出锚点列表
        {
          "seq": 0,
          // 锚点序号（从左到右）
          "numOfConnectedEdges": 0,
          // 连接边数量（任意整数）
          "targetAnchors": [
            // 当numOfConnectedEdges>0时存在
            {
              "nodeName": "string",
              // 目标节点名称
              "nodeMark": 0,
              // 目标节点标记
              "seq": 0
              // 目标锚点序号
            }
          ]
        }
      ]
    }
  ]
}